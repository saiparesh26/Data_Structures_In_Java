package com.saiparesh.adt;

public class Counter {

	private static int count = 0;
	private String str;
	public Counter(String str) {		
		this.str = str;
	}
	
	public void increment() {
		count++;
	}
	
	public int getCurrentValue() {
		return count;
	}
	
	@Override
	public String toString() {
		
	}
}
